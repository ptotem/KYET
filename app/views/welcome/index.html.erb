        <%= link_to "Edit", "#", class: "btn btn-warning span12", style: "color:black; margin-bottom:5px" %>
        <%= link_to "Logout", "#", class: "btn btn-danger span12" %>
      </div>
      <div class="span7">
        <div class="row-fluid" style="text-align: center">
          <div class="span3 blocked">
