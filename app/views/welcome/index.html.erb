<div id="main" role="main" class="container-fluid">

<div class="contained">
<!-- aside -->
<aside>

  <!-- Left Menu -->

  <div class="sidebar-nav-fixed">

    <div style="text-align: center;">
      <%= image_tag "/assets/img/logo.png" %>
    </div>
    <br>
    <ul class="menu" id="accordion-menu-js">
      <li class="current">
        <a href="/"><i class="icon-home"></i>Home</a>
      </li>
      <li class="">
        <a href="javascript:void(0);"><i class="icon-pencil"></i>Today's Quiz</a>
      </li>
      <li class="">
        <a href="javascript:void(0);"><i class="icon-briefcase"></i>Archives</a>
      </li>
      <li class="">
        <a href="javascript:void(0);"><i class="icon-flag"></i>LeaderBoard</a>
      </li>
      <li class="">
        <%= link_to destroy_user_session_path, method: :delete do %>
            <i class="icon-off"></i>Logout
        <% end %>
      </li>
      <li>

      </li>
    </ul>

    <br>

    <%= link_to "#", class: "btn btn-warning btn-large" do %>
        <sub><i class="livicon" data-name="question" data-size="60"></i></sub>
        <br>
        <br>
        Got a question? <br>Click here to understand how the know your ET Quiz works...
        <br>
        <br>
    <% end %>


  </div>

</aside>
